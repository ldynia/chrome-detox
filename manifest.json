{
  "name": "Detox",
  "description": "Breaking free from social media",
  "version": "1.0",
  "manifest_version": 3,
  "options_page": "app/html/options.html",
  "background": {
    "service_worker": "app/js/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "app/html/main.html",
    "default_icon": {
      "16": "app/images/get_started16.png",
      "32": "app/images/get_started32.png",
      "48": "app/images/get_started48.png",
      "128": "app/images/get_started128.png"
    }
  },
  "icons": {
    "16": "app/images/get_started16.png",
    "32": "app/images/get_started32.png",
    "48": "app/images/get_started48.png",
    "128": "app/images/get_started128.png"
  }
}